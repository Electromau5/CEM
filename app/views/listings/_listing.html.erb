  <div class = "container-fluid">
    <div class = "row">
		<% obj.each do |listing| %>
		  <div class = "col-xs-6 col-md-3">
		    <div class = "thumbnail">
             <%= link_to image_tag(listing.image.url(:medium)), listing %>
			  <div class = "listing_name">
			   		<p><%= listing.name %></p>
			   		<% if listing.seller %>
			   			<small>Seller: <%= link_to listing.seller.storename,listing.seller if listing.seller %></small>
			   		<% end %>
			   </div>
		     </div>
          </div>
		<% end %>
	</div>
  </div>	  
			  

		